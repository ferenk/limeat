{
  "name": "kcal",
  "version": "0.0.1",
  "description": "KCal calculation helper",
  "main": "server/index.js",
  "author": "Moricz, Ferenc",
  "license": "ISC",
  "scripts": {
    "start": "ts-node server/src",
    "debug": "node server/src/index.js     --inspect=127.0.0.1:11977",
    "debug-brk": "node server/src/index.js --inspect-brk=127.0.0.1:11977",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "preset": "ts-jest",
        "testMatch": [
          "<rootDir>/server/**/*.ts"
        ]
      }
    },
    "testMatch": [
      "<rootDir>/client/test/**/*.mjs"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "knex": "=0.95.15",
    "mongodb": "^3.7.3",
    "node": "^18.7.0",
    "path": "^0.12.7",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.9",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jquery": "^3.5.5",
    "@types/jsdom": "^20.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.6.2",
    "install": "^0.13.0",
    "jest": "^28.1.3",
    "jquery": "^3.5.1",
    "jsdom": "^20.0.0",
    "npm": "^8.15.1",
    "ts-jest": "^28.0.7",
    "typescript": "4.4"
  }
}
